{
  "configurations": {
    "simple_python: Launch": {
      "adapter": "vscode-python",
      "configuration": {
        "name": "simple_python: Launch",
        "type": "python",
        "request": "launch",
        "cwd": "${workspaceRoot}",
	"externalConsole": true,
	"stopAtEntry": true,
        "stopOnEntry": true,
        "debugOptions": [],
	"console": "externalTerminal",
        "program": "${file}",
        "logging": {
          "engineLogging": true
        }
	
      }
    }
  }
}
